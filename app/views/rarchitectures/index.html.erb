<p id="notice"><%= notice %></p>

<h1>Rarchitectures</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Link</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rarchitectures.each do |rarchitecture| %>
      <tr>
        <td><%= rarchitecture.description %></td>
        <td><%= rarchitecture.link %></td>
        <td><%= rarchitecture.location %></td>
        <td><%= link_to 'Show', rarchitecture %></td>
        <td><%= link_to 'Edit', edit_rarchitecture_path(rarchitecture) %></td>
        <td><%= link_to 'Destroy', rarchitecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rarchitecture', new_rarchitecture_path %>
